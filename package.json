    "postbuild": "echo 'Build completed successfully'",
    "start": "uvicorn main:app --host 0.0.0.0 --port $PORT",
    "dev": "uvicorn main:app --host 0.0.0.0 --port 8000 --reload"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aizamo-website.git"
  },
  "keywords": [
    "ai",
    "agency",
