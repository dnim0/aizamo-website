{
  "name": "aizamo-website",
  "version": "1.0.0",
  "description": "AIzamo AI Agency Website with GoHighLevel Integration",
  "main": "main.py",
  "scripts": {
    "heroku-prebuild": "echo 'Starting Heroku build process'",
